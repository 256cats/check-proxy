{
  "name": "check-proxy",
  "version": "0.2.0",
  "description": "Advanced Node proxy checker with socks and https support",
  "main": "build/index.js",
  "dependencies": {
    "bluebird": ">=3.1.1",
    "geoip-ultralight": "0.1.5",
    "lodash": "4.17.4",
    "node-libcurl": "https://github.com/256cats/node-libcurl#develop",
    "typescript": "2.4.2"
  },
  "scripts": {
    "test": "npm run build && mocha --timeout 5000",
    "postinstall": "npm run build",
    "build": "rm -rf ./build && ./node_modules/.bin/tsc && cp ./lib/interfaces.d.ts ./build/lib/interfaces.d.ts"
  },
  "keywords": [
    "check proxy",
    "test proxy"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/256cats/check-proxy.git"
  },
  "author": "Andrey E. <andrey@256cats.com> (256cats.com)",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/256cats/check-proxy/blob/master/LICENSE"
    }
  ],
  "devDependencies": {
    "@types/node": "8.0.19",
    "tslib": "1.7.1",
    "sinon": "^1.17.3",
    "mocha": "*",
    "proxyquire": ">=1.7.4"
  }
}
